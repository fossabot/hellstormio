{
  "version": "0.2.0",
  "configurations": [{
    "name": "Chrome Debug Config",
    "type": "chrome",
    "request": "launch",
    "url": "http://localhost",
    "webRoot": "${workspaceRoot}/src",
    "sourceMapPathOverrides": {
      "webpack:///src/*": "${webRoot}/*"
    },
    "timeout": 60000
  }]
}
